@PACKAGE_INIT@

find_package(mrpt_libros_bridge REQUIRED)

# Provide an alias target so consumers can do: target_link_libraries(... mrpt::ros2bridge)
if(NOT TARGET mrpt::ros2bridge)
  add_library(mrpt::ros2bridge ALIAS mrpt_libros_bridge::mrpt_libros_bridge)
endif()

# Make sure mrpt-maps mrpt-obs are also found:
include(CMakeFindDependencyMacro)

find_dependency(mrpt-maps REQUIRED)
find_dependency(mrpt-obs REQUIRED)
